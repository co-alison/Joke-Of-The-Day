{
    "name": "Joke Of The Day",
    "version": "0.0.1",
    "description": "Have a laugh with a joke of the day.",
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "manifest_version": 3,
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}